CREATE TABLE PANDA (SID NUMBER,SNAME CHAR(900),MATH NUMBER,SCI NUMBER);
INSERT INTO PANDA VALUES('1','ANIRUDH','88','90');
INSERT INTO PANDA VALUES('3',’LIVIE’,'55','97');
INSERT INTO PANDA VALUES('2','SATWIKA','79','99');
INSERT INTO PANDA VALUES('4','VISHNU','78','90');
INSERT INTO PANDA VALUES('5','KARTHIK','9','0');
INSERT INTO PANDA VALUES('6','CHALITH','88','90');
SELECT*FROM PANDA;
SELECT*FROM PANDA WHERE MATH<80;
SELECT*FROM PANDA WHERE SCI>90;
SELECT*FROM PANDA WHERE MATH<=79;
SELECT*FROM PANDA WHERE SCI=90;
SELECT MATH+SCI AS TOTAL FROM PANDA;
SELECT*FROM PANDA WHERE MATH!=3;
SELECT*FROM PANDA WHERE SCI^=90;
SELECT*FROM PANDA WHERE SCI>=98;
SELECT*FROM PANDA WHERE MATH IN(80,88,90);
SELECT*FROM PANDA WHERE SCI NOT IN(100);
SELECT*FROM PANDA WHERE MATH BETWEEN 70 AND 80;
SELECT*FROM PANDA WHERE SCI NOT BETWEEN 90 AND 97;
SELECT*FROM PANDA WHERE SNAME LIKE 'A%';
SELECT*FROM PANDA WHERE SNAME NOT LIKE '%R';
DROP TABLE PANDA;
